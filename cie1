#include <stdio.h>
#include <omp.h>

int main() {
    // Start parallel region
    #pragma omp parallel
    {
        int thread_id = omp_get_thread_num();       // Get thread ID
        int team_size = omp_get_num_threads();      // Get total number of threads

        printf("Hello from thread %d of %d threads\n", thread_id, team_size);
    }
    return 0;
}
